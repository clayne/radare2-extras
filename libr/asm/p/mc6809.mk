OBJ_MC6809=arch_mc6809.o

STATIC_OBJ+=${OBJ_MC6809}

TARGET_MC6809=arch_mc6809.${LIBEXT}

ALL_TARGETS+=${TARGET_MC6809}

$(OBJ_MC6809): %.o : %.c
	$(CC) -c $(CFLAGS) $< -o $@

${TARGET_MC6809}: ${OBJ_MC6809}
	${CC} $(call libname,arch_mc6809) ${LDFLAGS} \
		${CFLAGS} -o ${TARGET_MC6809} ${OBJ_MC6809}
